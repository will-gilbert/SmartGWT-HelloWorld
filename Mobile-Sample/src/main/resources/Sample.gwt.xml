<?xml version="1.0" encoding="UTF-8"?>

<module rename-to="app">

    <inherits name="com.smartgwt.mobile.SmartGwtMobile"/>

    <!-- Class names: pretty, debug, stable, stable-shorttype, stable-notype -->
    <set-configuration-property name="CssResource.style" value="stable-notype" />

    <!-- Specify the app entry point class -->
    <entry-point class="com.mycompany.client.Sample"/>

    <!-- Limit the target browsers to WebKit (Safari,Chrome) and later Firefoxes  -->
    <set-property name="user.agent" value="safari"/>

    <!-- Specify the path(s) containing Java code to be compiled into JavaScript  -->
    <source path='com/mycompany/client'/>


</module>
